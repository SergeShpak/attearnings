{
  "name": "attearnings",
  "version": "1.0.0",
  "description": "Calculate the roller coaster daily earnings",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "make-pretty": "npx prettier --write .",
    "test:unit": "nyc --all --exclude '**/*.test.[tj]s' mocha --config .mocharc.json --exclude 'src/tests/**/*.test.ts' 'src/**/*.test.ts'",
    "test:fs": "nyc --all --exclude '**/*.test.[tj]s' mocha --config .mocharc.json 'src/tests/fs-tests/*.test.ts'",
    "test:all": "nyc --all --exclude '**/*.test.[tj]s' mocha --config .mocharc.json 'src/**/*.test.ts'",
    "clean": "find src -name \"*.js\" -exec rm {} \\;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergeyShpak/attearnings.git"
  },
  "author": "Serge Shpak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SergeyShpak/attearnings/issues"
  },
  "homepage": "https://github.com/SergeyShpak/attearnings#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/yargs": "^17.0.10",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "yargs": "^17.4.0"
  }
}